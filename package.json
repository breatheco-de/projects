{
  "name": "gatsby-starter-default",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@breathecode/ui-components": "^1.1.62",
    "bc-react-notifier": "^1.1.6",
    "gatsby": "^2.19.16",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-google-tagmanager": "^2.0.15",
    "gatsby-plugin-manifest": "^2.3.1",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.5.1",
    "gatsby-plugin-sitemap": "^2.2.4",
    "gatsby-source-filesystem": "^2.2.1",
    "gatsby-transformer-sharp": "^2.4.1",
    "node-fetch": "^2.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-player": "^1.11.0"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "now-build": "npm run build",
    "start": "gatsby develop -p 3000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "serve": "gatsby serve",
    "generate-projects": "php ./json/generate.php",
    "deploy": "npm run generate-projects && now --prod",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "pre-commit": [
    "generate-projects"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
